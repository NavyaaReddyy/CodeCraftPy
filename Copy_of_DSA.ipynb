      "source": [
        "def print_pyramid(N):\n",
        "    for i in range(1, N + 1):\n",
        "        # Print leading spaces\n",
        "        print(\" \" * (N - i), end=\"\")\n",
        "        # Print stars\n",
        "        print(\"* \" * i)\n",
        "\n",
        "# Example usage:\n",
        "N = 3\n",
        "print_pyramid(N)\n"
      ]
